<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "pl.bie.entity.RecordEntity" table = "combinedata">

        <meta attribute = "class-description">
            This class contains the employee detail.
        </meta>

        <id name = "id" type = "int" column = "id">
            <generator class="native"/>
        </id>

        <property name = "voivodeshipName" column = "voivodeshipName" type = "string"/>
        <property name = "category" column = "category" type = "string"/>
        <property name = "year" column = "year" type = "int"/>
        <property name = "value" column = "value" type = "float"/>

    </class>
</hibernate-mapping>